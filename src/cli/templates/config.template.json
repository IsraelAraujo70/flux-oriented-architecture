{
  "server": {
    "port": 3000,
    "cors": {
      "origin": "${APP_BASE_URL}",
      "credentials": true,
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "allowedHeaders": ["Content-Type", "Authorization"]
    }
  },
  "paths": {
    "actions": "src/actions",
    "flux": "src/flux"
  },
  "logging": {
    "level": "info"
  }
}
